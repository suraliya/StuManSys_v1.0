<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 
    <class name="com.persistence.user.Students" table="student">
 
        <id name="stu_id" column="stu_id">
            <generator class="native" />
        </id>
 
        <property name="firstname">
            <column name="firstname" length="16" not-null="true" />
        </property>
 
        <property name="lastname">
            <column name="lastname" length="16" not-null="true" />
        </property>
        
        <property name="dob">
            <column name="dob" length="16" not-null="true" />
        </property>
        
        <property name="nic">
            <column name="nic" length="16" not-null="true" />
        </property>
        
        <property name="gender">
            <column name="gender" length="16" not-null="true" />
        </property>
        
        <property name="contactno">
            <column name="contactno" length="16" not-null="true" />
        </property>
        
        <property name="email">
            <column name="email" length="16" not-null="true" />
        </property>
        
        <property name="address">
            <column name="address" length="16" not-null="true" />
        </property>
        
        <property name="course">
            <column name="course" length="16" not-null="true" />
        </property>
  
    </class>
</hibernate-mapping>